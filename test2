digraph finite_state_machine {
  rankdir=LR;
  size="8,5"
  node [shape = circle];
  a -> e [ label = "5" ]; 
  a -> g [ label = "20" ]; 
  c -> g [ label = "5" ]; 
  c -> e [ label = "10" ]; 
  c -> f [ label = "10" ]; 
  c -> d [ label = "10" ]; 
  b -> e [ label = "10" ]; 
  b -> f [ label = "10" ]; 
}